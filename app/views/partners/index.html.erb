<div class="admin_wrapper">

    <div class="admin-header">
      <%=render 'bookings/navbar' %>
    </div>

<div class="container">
<h1>Our Partners</h1>

<table class='table table-striped table_style'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th>State</th>
      <th>City</th>
      <th>Office location</th>
      <th>Specializations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= partner.first_name %></td>
        <td><%= partner.last_name %></td>
        <td><%= partner.gender %></td>
        <td><%= partner.state %></td>
        <td><%= partner.city %></td>
        <td><%= partner.office_location %></td>
        <td><%= partner.specializations %></td>
        <td><%= link_to 'Details', partner %></td>
        <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
        <td><%= link_to 'Delete', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div id='pagination_div'>
  <%#will_paginate @Subscriptions, renderer: BootstrapPagination::Rails %>
</div>

<br>

<%= link_to 'New Partner', new_partner_path %>


</div>
</div>

<%=render "shared/footer_scroll"%>